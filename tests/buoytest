#!/bin/bash
if [ -z "$1" -o "$1" == 'short' ]; then
  time ../bin/buoyboy.py 46022 "10/20/2008 00:00:00" "10/20/2008 02:00:00"
elif [ "$1" == 'long' ]; then
  time ../bin/buoyboy.py 46022 "10/20/2008 00:00:00" "11/20/2008 00:00:00"
elif [ "$1" == 'JSON' ]; then
  time ../bin/buoyboy.py 46022 "10/20/2008 00:00:00" "10/20/2008 02:00:00" windData.json waveData.json
elif [ "$1" == 'DB' ]; then
  time ../bin/buoyboy.py --format database 46022 "10/20/2008 00:00:00" "10/20/2008 02:00:00"
elif [ "$1" == 'matlab' ]; then
  time ../bin/buoyboy.py --format matlab 46022 "10/20/2008 00:00:00" "10/20/2008 02:00:00"
fi
